{
  "name": "joplin-macos-native-theme",
  "version": "0.0.1",
  "description": "Native looking macOS theme for note taking app Joplin",
  "keywords": [
    "joplin",
    "themes",
    "css",
    "macos"
  ],
  "author": "Andre Jilderda",
  "license": "MIT",
  "scripts": {
    "start": "node-sass --include-path scss scss/main.scss   dist/main.css -watch",
