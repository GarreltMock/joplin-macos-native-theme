{
  "name": "joplin-macos-native-theme",
  "version": "1.1.1",
  "description": "Native looking macOS theme for note taking app Joplin",
  "keywords": [
    "joplin",
    "themes",
    "css",
    "macos"
  ],
  "author": "Andre Jilderda",
  "license": "MIT",
  "scripts": {
    "start": "sass --load-path scss scss/macos-theme-for-joplin.scss   dist/macos-theme-for-joplin.css --watch",
    "serve": "http-server dist -c-1 -o macos-theme-for-joplin.css",
    "build": "sass --load-path scss scss/macos-theme-for-joplin.scss   dist/macos-theme-for-joplin.css --no-source-map ",
    "clean": "rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "http-server": "^0.12.3",
    "sass": "^1.35.2"
  }
}