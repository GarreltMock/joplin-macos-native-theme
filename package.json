    "build": "node-sass --include-path scss scss/main.scss   dist/main.css",
    "clean": "rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "http-server": "^0.12.3",
    "node-sass": "^5.0.0"
  }
}